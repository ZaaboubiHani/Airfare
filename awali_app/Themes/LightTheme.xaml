<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Background Colors -->
    <SolidColorBrush x:Key="PrimaryBackgroundColor" Color="#DCE2F9"/>
    <SolidColorBrush x:Key="SecundaryBackgroundColor" Color="#FCFCFC"/>

    <!-- Primary Colors -->
    <SolidColorBrush x:Key="PrimaryBlueColor" Color="#5B7FFF"/>
    <SolidColorBrush x:Key="PrimaryGrayColor" Color="#BFBFC4"/>

    <!-- Secundary Colors -->
    <SolidColorBrush x:Key="SecundaryBlueColor" Color="#CEE8FB"/>
    <SolidColorBrush x:Key="SecundaryGrayColor" Color="#EEEEEE"/>

    <!-- Typograhy Colors -->
    <SolidColorBrush x:Key="TextPrimaryColor" Color="#020F16"/>
    <SolidColorBrush x:Key="TextSecundaryColor" Color="#555"/>

    <!-- Icons Colors -->
    <SolidColorBrush x:Key="ActionIconColor" Color="#56588B"/>
    <!-- Lgo palett Colors -->
    <SolidColorBrush x:Key="LightOrangeColor" Color="#FFB703"/>
    <SolidColorBrush x:Key="VividOrangeColor" Color="#FD9E02"/>
    <SolidColorBrush x:Key="DarkOrangeColor" Color="#FB8500"/>
    <SolidColorBrush x:Key="LightBlueColor" Color="#8ECAE6"/>
    <SolidColorBrush x:Key="VividBlueColor" Color="#219EBC"/>
    <SolidColorBrush x:Key="DarkBlueColor" Color="#023047"/>
</ResourceDictionary>